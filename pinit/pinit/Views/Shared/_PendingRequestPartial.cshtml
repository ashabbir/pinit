@model IEnumerable<pinit.Models.UserFriend>


@if (Model.Count() > 0)
{
    <div class="panel panel-warning">
        <div class="panel-heading">
            <h3 class="panel-title">Pending Request</h3>
        </div>
        <div class="panel-body">
            <ul class="list-group">
                @foreach (var item in Model)
                {
                    <li class="list-group-item">@Html.Label(item.PersonalInfo.FirstName)  @Html.Label(item.PersonalInfo.LastName)</li>
                }
            </ul>
        </div><!-- /.panel-body -->
        <div class="panel-footer">
            <em class="text-info">These are the request you sent out and people have not accepted or rejected them</em>
        </div>
    </div><!-- panel panel-default-->
}