@model IEnumerable<pinit.Data.Pin>
@if (Model.Count() > 0)
    {
<ul class="list-inline">
    
        @foreach (var item in Model)
        {
        <li>
            <p>
                @Html.ActionBoard("Details", "boards", new { id = item.PinId }, item.Picture.ImageUrl , "pin")
            </p>
        </li>
        }
   
</ul>

    }
else
{
    <p>You have no pins</p>
}

